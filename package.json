{
  "name": "mmp-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Maker Management Platform - Monorepo",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "clean": "turbo run clean && rm -rf node_modules .turbo",
    "docker:build": "docker build -f docker/Dockerfile -t mmp:latest .",
    "docker:run": "docker-compose -f docker/docker-compose.yml up"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "turbo": "^2.0.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
